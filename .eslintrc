{
  "env": {
    "jest": true
  },
  "extends": ["@antfu"],
  "plugins": ["jest"],
  "rules": {
    "@typescript-eslint/no-unused-vars": ["error", { "argsIgnorePattern": "^_" }],
    "react/react-in-jsx-scope": "off",
    "no-restricted-imports": [
      "error",
      {
        "paths": ["unocss"]
      }
    ]
  },
  "overrides": [
    {
      "files": [
        "playground/**/*.*"
      ],
      "rules": {
        "no-restricted-imports": "off"
      }
    }
  ]
}
